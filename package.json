{
  "name": "graphql-api-example",
  "version": "1.0.0",
  "main": "./dist/server.js",
  "scripts": {
    "setup": "yarn install",
    "start": "node dist/server.js",
    "serve:live": "ts-node-dev --respawn src/server.ts",
    "clean": "rimraf dist",
    "test": "jest",
    "test:coverage": "npm run test -- --collectCoverage",
    "build": "npm run clean && npm run build:compile",
    "build:compile": "tsc -t ES5 --lib es6 --outDir ./dist"
  },
  "license": "MIT",
  "dependencies": {
    "apollo-server": "2.10.1",
    "graphql": "14.6.0",
    "koa-bodyparser": "4.2.1",
    "koa-router": "8.0.6",
    "reflect-metadata": "0.1.13",
    "ts-node": "8.6.2",
    "ts-node-dev": "1.0.0-pre.44",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.7.3"
  },
  "devDependencies": {
    "@types/jest": "24.0.25",
    "@types/node": "13.1.7",
    "codecov": "3.6.1",
    "cross-env": "7.0.0",
    "jest": "24.9.0",
    "mocha": "7.0.1",
    "npm-run-all": "4.1.5",
    "nyc": "15.0.0",
    "rimraf": "3.0.1",
    "shx": "0.3.2",
    "travis": "0.1.1",
    "ts-jest": "25.1.0"
  },
  "resolutions": {
    "**/**/set-value": "^2.0.1",
    "**/**/handlebars": "^4.5.3",
    "**/**/https-proxy-agent": "^2.2.3",
    "**/**/lodash": "^4.17.12"
  }
}
